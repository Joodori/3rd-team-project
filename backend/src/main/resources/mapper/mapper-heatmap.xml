<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lx.edu.levelup.feature.heatmap.HeatMapRepository">

    <select id="findRecentLogs" parameterType="java.util.Map" resultType="lx.edu.levelup.feature.heatmap.HeatMapVO">
        SELECT latitude, longitude
        FROM crowd_log
        WHERE log_time BETWEEN #{startTime} AND #{endTime}
    </select>


</mapper>